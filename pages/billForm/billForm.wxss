.active{
  color:#006bff;
  border-bottom: 4rpx solid #006bff;

}


.table {
  border: 0px solid darkgray;
 }
 .tr {
  display: flex;
  width: 100%;
  justify-content: center;
  height: 3rem;
  align-items: center;
 }
 .td {
  width:40%;
  justify-content: center;
  text-align: center;
 }
 .bg-w{
  background: snow;
 }
 .bg-g{
  background: #E6F3F9;
 }
 .bg-gg{
  background: snow;
 }
 .th {
  width: 40%;
  justify-content: center;
  background: #006bff;
  color: #fff;
  display: flex;
  height: 3rem;
  align-items: center;
 }
 .title{
   font-size: 30rpx;
   color:#696969;
   text-align: center;
   padding:20rpx 0 20rpx 0;
 }
.sum
{
  padding:20rpx 20rpx 50rpx 0;
  text-align: right;
  border-top:1px solid #696969;
  font-size: 40rpx;
}
.btn{
  padding-bottom: 200rpx;
}
.btn button{
  position: absolute;
  width: 300rpx;
  height: 85rpx;
  text-align: center;
  left: 200rpx;
}
